{
  "root" : {
    "appender": [
      {
        "level": "error",
        "stream" : "console"
      },
      {
        "level": "error",
        "stream" : "file",
        "path" : "./logs",
        "fileName" : "%HostName%_root_%Y%m%d_%H%M%S_%N.log.log",
        "maxFileByte" : 1024000,
        "rollSecond" : 3600
      }
    ]
  },

  "test1": {
    "level" : "trace",
    "appender": [
      {
        "level": "trace",
        "stream" : "console"
      },
      {
        "level": "trace",
        "stream" : "file",
        "path" : "./logs",
        "fileName" : "test1_%Y%m%d_%H%M%S_%N.log",
        "maxFileByte" : 1024000,
        "rollSecond" : 3600
      }
    ]
  }
}