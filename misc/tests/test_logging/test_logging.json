{
  "root" : {
    "appender": [
      {
        "stream" : "console",
        "level": "error"
      },
      {
        "stream" : "file",
        "level": "error",
        "path" : "./logs",
        "fileName" : "%HostName%_root_%Y%m%d_%H%M%S_%N.log.log",
        "maxFileByte" : 1024000,
        "rollSecond" : 3600
      }
    ]
  },

  "test1": {
    "level" : "trace",
    "appender": [
      {
        "stream" : "console",
        "level": "trace",
        "color" : true,
        "unorder" : false
      },
      {
        "stream" : "file",
        "format" : "normal",
        "level": "trace",
        "path" : "./logs",
        "fileName" : "test_normal_%Y%m%d_%H%M%S_%N.log",
        "maxFileByte" : 1024000,
        "rollSecond" : 3600,
        "unorder" : false
      },
      {
        "stream" : "file",
        "format" : "xml",
        "level": "trace",
        "path" : "./logs",
        "fileName" : "test_xml_%Y%m%d_%H%M%S_%N.log",
        "maxFileByte" : 1024000,
        "rollSecond" : 3600,
        "unorder" : false
      },
      {
        "stream" : "file",
        "format" : "json",
        "level": "trace",
        "path" : "./logs",
        "fileName" : "test_json_%Y%m%d_%H%M%S_%N.log",
        "maxFileByte" : 1024000,
        "rollSecond" : 3600,
        "unorder" : false
      },
      {
        "stream" : "tracer",
        "level": "trace",
        "unorder" : false
      },
      {
        "stream" : "syslog",
        "level": "trace",
        "url" : "localhost",
        "port" : 514,
        "unorder" : false
      }
    ]
  }
}